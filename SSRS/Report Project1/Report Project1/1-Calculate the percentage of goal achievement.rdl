  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="DataSource1">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=.;Initial Catalog="Darina Exam"</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>06770e85-e412-4ab2-bb49-c626b81c9d1a</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT  


	T.[year],
    T.[month],
    SUM(cast(SID.[TotalPrice] as float)) AS TotalSales,
    T.[target] AS TargetAmount,
    (SUM(cast(SID.[TotalPrice] as float)) / cast(T.[target] as float))*100  AS AchievementPercentage
		
FROM 
   [dbo].[Sale InvoceDetail] SID
JOIN 
    [Darina Exam ].[dbo].[DimDate] DD 
	
	ON SID.[DateID] = DD.[Shamsi Date Number]

JOIN 

    [dbo].[Target ] T ON DD.[Shamsi Year] = T.[year] AND DD.[Shamsi Month] = T.[month]
GROUP BY 
    T.[year], T.[month], T.[target]
ORDER BY 
    T.[year], T.[month];</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="month">
          <DataField>month</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="TargetAmount">
          <DataField>TargetAmount</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="AchievementPercentage">
          <DataField>AchievementPercentage</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
