  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>WITH MonthlySales AS (
    SELECT 
        DD.[Shamsi Year] AS Year,
        DD.[Shamsi Month Name] AS Month,
        SUM(cast (SID.[TotalPrice] as float)) AS MonthlySales
    FROM 
        [dbo].[Sale InvoceDetail] SID
    JOIN 
        [dbo].[DimDate]  DD ON SID.[DateID] = DD.[Shamsi Date Number]
    GROUP BY 
        DD.[Shamsi Year], DD.[Shamsi Month Name]
)
SELECT 
    Year, 
    Month, 
    SUM(MonthlySales) OVER (PARTITION BY Year ORDER BY Month) AS CumulativeSales
FROM 
    MonthlySales
ORDER BY 
    Year, Month;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CumulativeSales">
          <DataField>CumulativeSales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
