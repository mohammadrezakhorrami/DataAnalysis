  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource4</DataSourceName>
        <CommandText>SELECT 
    DD.[Shamsi Year] AS Year,
    DD.[Shamsi Month] AS Month,
    SUM(cast (SID.[TotalPrice] as float)) AS CurrentYearSales,
    LAG(SUM(cast (SID.[TotalPrice] as float)), 1) OVER (PARTITION BY DD.[Shamsi Month] ORDER BY DD.[Shamsi Year]) AS PreviousYearSales,
    ((SUM(cast (SID.[TotalPrice] as float)) - LAG(SUM(cast (SID.[TotalPrice] as float)), 1) OVER (PARTITION BY DD.[Shamsi Month] ORDER BY DD.[Shamsi Year])) / LAG(SUM(cast (SID.[TotalPrice] as  float)), 1) OVER (PARTITION BY DD.[Shamsi Month] ORDER BY DD.[Shamsi Year])) * 100 AS YoYGrowth
FROM 
   [dbo].[Sale InvoceDetail] SID
JOIN 
    [Darina Exam].[dbo].[DimDate] DD ON SID.[DateID] = DD.[Shamsi Date Number]
GROUP BY 
    DD.[Shamsi Year], DD.[Shamsi Month]
ORDER BY 
    DD.[Shamsi Year], DD.[Shamsi Month];</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="CurrentYearSales">
          <DataField>CurrentYearSales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="PreviousYearSales">
          <DataField>PreviousYearSales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="YoYGrowth">
          <DataField>YoYGrowth</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
