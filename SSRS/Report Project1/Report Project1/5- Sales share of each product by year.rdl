  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource5</DataSourceName>
        <CommandText>WITH ProductSales AS (
    SELECT 
        DD.[Shamsi Year] AS Year,
        PT.[category],
        SUM(CAST(SID.[TotalPrice] AS FLOAT)) AS Sales
    FROM 
        [dbo].[Sale InvoceDetail] SID
    JOIN 
        [dbo].[DimDate] DD ON SID.year = DD.[shamsi year]
    JOIN 
        [dbo].[Product Table] PT ON SID.[GoodsID] = PT.[Product_ID]
    GROUP BY 
        DD.[Shamsi Year], PT.[category]
),
CategorySalesShare AS (
    SELECT
        PS.Year,
        PS.category,
        (PS.Sales / SUM(PS.Sales) OVER (PARTITION BY PS.Year)) * 100 AS ProductShare
    FROM 
        ProductSales PS
)
SELECT 
    Year,
    category,
    SUM(ProductShare) AS TotalProductShare
FROM 
    CategorySalesShare
GROUP BY 
    Year, category
ORDER BY 
    Year, TotalProductShare DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalProductShare">
          <DataField>TotalProductShare</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
